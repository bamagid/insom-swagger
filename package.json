{
  "name": "insomswagger",
  "version": "1.1.1",
  "description": "A command-line tool for converting JSON exported by Insomnia to Swagger format, as well as converting a swagger-formatted json file into a swagger-formatted annotation to facilitate documentation generation.",
  "main": "./insomswagger.mjs",
  "bin": "./insomswagger.mjs",
  "scripts": {
     "insomswagger": "node ./insomswagger.mjs"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/bamagid/insom-swagger.git"
  },
  "keywords": [
    "insomnia",
    "swagger",
    "docs",
    "documentation"
  ],
  "author": "Magid Ba",
  "license": "SEE LICENSE IN License.md",
  "homepage": "https://github.com/bamagid/insom-swagger#readme",
  "bugs": {
    "url": "https://github.com/bamagid/insom-swagger/issues"
  },
  "engines": {
    "node": ">=12.0.0"
  },
  "dependencies": {
    "insomswagger": "^1.0.4",
    "open": "^10.0.1"
  }
}
